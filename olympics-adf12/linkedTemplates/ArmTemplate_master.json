{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "olympics-adf12"
		},
		"jsonsource_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'jsonsource'"
		},
		"sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sink'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"jsonsource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://olympicsadls2025.dfs.core.windows.net/"
		},
		"sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://olympicsadls2025.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "olympics-adf12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"jsonsource_accountKey": {
						"value": "[parameters('jsonsource_accountKey')]"
					},
					"sink_accountKey": {
						"value": "[parameters('sink_accountKey')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"jsonsource_properties_typeProperties_url": {
						"value": "[parameters('jsonsource_properties_typeProperties_url')]"
					},
					"sink_properties_typeProperties_url": {
						"value": "[parameters('sink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}